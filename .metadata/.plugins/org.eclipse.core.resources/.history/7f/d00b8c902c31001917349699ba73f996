package br.com.marquise.centrofashion.model;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import br.com.marquise.model.BaseEntity;

@Entity
@Table(name = "APPS.PN_LOCATIONS_ALL")
@NamedQueries({
	@NamedQuery(name="OFFICE_FROM_PN_LOCATIONS_ALL", query=""
			+ "SELECT p " + 			
			" FROM PnLocationsAll p " + 
			" WHERE p.locationCode LIKE :locationCode AND p.attribute2 IS NOT NULL"),
			
	@NamedQuery(name="FROM_PN_LOCATIONS_ALL_BY_LOCATION_OFFICE", query=""
			+ " select p " + 
			"  FROM PnLocationsAll p " + 
			" WHERE p.locationCode LIKE :locationCode " + 
			"   AND p.attribute2 IS NOT NULL " + 
			"   AND p.office = :office "),
	
	@NamedQuery(name="OFFICEE_FROM_PN_LOCATIONS_ALL", query="Select pr"
					+
					" FROM PnLocationAll pr" +
					" WHERE pr.locationCode LIKE : locationCode " +
					" AND pr.atribute2 IS NOT NULL " + 
					" AND pr.office = :office ")
	
})
public class PnLocationsAll {
	@Id
	@Column(name="ROWID")
	private String rowid;
	
	@Column(name="OFFICE")
	private String office;
	
	@Column(name="ATTRIBUTE1")
	private String attribute1;
	
	@Column(name="ATTRIBUTE2")
	private String attribute2;
	
	@Column(name="ATTRIBUTE3")
	private String attribute3;
	
	@Column(name="LOCATION_CODE")
	private String locationCode;
	
	@Column(name="USABLE_AREA")
	private double usableArea;

	public String getRowid() {
		return rowid;
	}

	public String getOffice() {
		return office;
	}

	public String getAttribute2() {
		return attribute2;
	}

	public String getLocationCode() {
		return locationCode;
	}

	public void setRowid(String rowid) {
		this.rowid = rowid;
	}

	public void setOffice(String office) {
		this.office = office;
	}

	public void setAttribute2(String attribute2) {
		this.attribute2 = attribute2;
	}

	public void setLocationCode(String locationCode) {
		this.locationCode = locationCode;
	}

	public String getAttribute1() {
		return attribute1;
	}

	public String getAttribute3() {
		return attribute3;
	}

	public double getUsableArea() {
		return usableArea;
	}

	public void setAttribute1(String attribute1) {
		this.attribute1 = attribute1;
	}

	public void setAttribute3(String attribute3) {
		this.attribute3 = attribute3;
	}

	public void setUsableArea(double usableArea) {
		this.usableArea = usableArea;
	}
	
	
	 
}
