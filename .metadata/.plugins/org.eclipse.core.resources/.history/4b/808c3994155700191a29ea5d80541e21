package entities;

public class Triangle {

	public double a , b, c;

	
	public double area() {
		
		double p = (a + b + c ) / 2.0;
		double result  = Math.sqrt(p * (p - a) * (p - b) * (p - c));
		
		
	}
	
	
	
	
	
	/*double p = (x.a + x.b + x.c) / 2.0;
	double areaX = Math.sqrt(p * (p - x.a) * (p - x.b) * (p - x.c));

	p = (y.a + y.b + y.c) / 2.0;
	double areaY = Math.sqrt(p * (p - y.a) * (p - y.b) * (p - y.c));*/

}
