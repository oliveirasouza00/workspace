package br.com.marquise.centrofashion.model;

import javax.persistence.AttributeOverride;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.JoinColumn;
import javax.persistence.OneToOne;
import javax.persistence.SequenceGenerator;
import javax.persistence.Table;

import org.hibernate.engine.loading.internal.LoadingCollectionEntry;

import br.com.marquise.model.BaseEntity;
import br.com.marquise.model.Usuario;

@Entity
@Table(name = "JVS.CF_CAIXA_LIDERES")
@SequenceGenerator(name = "ENTITY_SEQUENCE", sequenceName = "JVS.CF_CAIXA_LIDERES_S",  allocationSize=1 )
@AttributeOverride(name = "cod", column = @Column(name = "COD"))
public class CfCaixaLideres extends BaseEntity {

	@OneToOne
	@JoinColumn(name = "USU_COD")
	private Usuario usuario;
	
	@Column(name = "IS_CASH")
	private Long isCash = 0L;

	public Usuario getUsuario() {
		return usuario;
	}

	public Long getIsCash() {
		return isCash;
	}

	public void setUsuario(Usuario usuario) {
		this.usuario = usuario;
	}

	public void setIsCash(Long isCash) {
		this.isCash = isCash;
	}

}
